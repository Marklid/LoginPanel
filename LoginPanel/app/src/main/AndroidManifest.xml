<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="in.careurhealth.careurhealth">

    <application
        android:allowBackup="true"
        android:icon="@drawable/icon"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".StartActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".PatientLogin"/>
        <activity android:name=".DoctorLogin" />
        <activity android:name=".DoctorForgotPassword" />
        <activity android:name=".PatientForgotPassword"/>
        <activity android:name=".DoctorView" />
        <activity android:name=".PatientSignUp"/>
        <activity android:name=".DoctorSignUp"/>
        <activity android:name=".PatientView"/>
    </application>

</manifest>